{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            
            "program": "D:/Dropbox/source/IdeaForge/mockup/gs1/bin/Debug/net8.0//gs1.dll>",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Python: Flask",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/app.py",  // Your Python app entry point
            "args": ["run"],
            "jinja": true,
            "console": "integratedTerminal",
            "env": {
              "FLASK_APP": "${workspaceFolder}/app.py"
            }
          },
          {
            "name": "Edge: Launch",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:5000", // Adjust the port based on your Flask app
            "webRoot": "${workspaceFolder}/",
            "skipFiles": ["<eval>/**"],
            "sourceMaps": true,
            "trace": true
          },
        
        {
            "name": "Proxy test Python Debugger: Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "trials/proxytest.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run"
            ],
            "jinja": true,
            "autoStartBrowser": false,
            "justMyCode": false
        }
    ],
    "compounds": [
    {
      "name": "Python + Edge",
      "configurations": ["Python: Flask", "Edge: Launch"]
    }
  ],
}